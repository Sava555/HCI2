<Window x:Class="HCI2.OdabirMape"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HCI2"
        mc:Ignorable="d"
        Title="OdabirMape" Height="300" Width="400">
    <Window.CommandBindings>
        <CommandBinding Command="ApplicationCommands.Help" Executed="HelpBinding_Executed"></CommandBinding>
    </Window.CommandBindings>
    <Grid TextElement.FontSize="18">
        <ListView Margin="10" >
            <ListViewItem PreviewKeyDown="Mapa1_Selected" MouseDoubleClick="Mapa1_Selected_click" Focusable="True">Mapa1</ListViewItem>
            <ListViewItem PreviewKeyDown="Mapa2_Selected" MouseDoubleClick="Mapa2_Selected_click" Focusable="True">Mapa2</ListViewItem>
            <ListViewItem PreviewKeyDown="Mapa3_Selected" MouseDoubleClick="Mapa3_Selected_click" Focusable="True">Mapa3</ListViewItem>
            <ListViewItem PreviewKeyDown="Mapa4_Selected" MouseDoubleClick="Mapa4_Selected_click" Focusable="True">Mapa4</ListViewItem>
        </ListView>
    </Grid>
    

</Window>
